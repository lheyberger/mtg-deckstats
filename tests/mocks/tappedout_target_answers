                function changeBodyPadding() {
                   var footerHeight = $("#footer").outerHeight() + 20;
                   $("#body").css("padding-bottom", footerHeight + "px");
                }
                // Set div#body padding on load
                $(window).ready(function(e) {
                   changeBodyPadding();
